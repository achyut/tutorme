<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uta.edu.tutorme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.CategoryActivity" />
        <activity android:name=".activities.SubcategoryActivity" />
        <activity android:name=".activities.PopulateCategoryActivity" />
        <activity android:name=".activities.PopulateSubCategoryActivity"></activity>


        <meta-data android:name="DATABASE" android:value="tutorme.db" />
        <meta-data android:name="VERSION" android:value="1" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="uta.edu.tutorme.models" />

    </application>

</manifest>
